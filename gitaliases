alias ga='git add'
alias gs='git status'
alias gc='git commit'
alias gam='git commit --amend'
alias gm='git merge'
alias go='git checkout'
alias gb='git branch'
alias gri='git rebase -i'
alias gpr='git pull --rebase && git push'
alias gh='git log -n 5 --graph'
alias gd='git diff HEAD'
alias gp='git pull'
alias gl='git log --graph'
alias gpm='git pull --rebase origin master'
alias gdn='git diff --name-only'
alias grh='git reset HEAD --hard'
alias gcp='git cherry-pick'

alias gb.commits='for e in $(git branch | cut -c 3- | grep RASP); do echo $e; git log $e -n 1 --oneline; echo ""; done'
alias gb.pushremote='branch=$(git branch | grep "\* " | cut -c3-) && git push -u seiv $branch'
alias gb.dropbranches="echo 'git checkout master && for b in \$(git branch | grep -v master | cut -c3-); do echo $b; gb -D $b; done'"
alias gb.updateremote='branch=$(git branch | grep "\* " | cut -c3-) && git pull origin $branch && git push seiv $branch'
